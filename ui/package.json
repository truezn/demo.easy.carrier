{
  "name": "demo.easy.carrier",
  "version": "0.0.1",
  "private": true,
  "description": "Demo for easy carrier",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^2.14.1",
    "@sap/ux-ui5-tooling": "1",
    "@sap/ux-ui5-fe-mockserver-middleware": "1",
    "@sap/approuter": "^10.4.2",
    "@sap/html5-repo-mock": "^2.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2"
  },
  "scripts": {
    "preinstall": "npm install -g @ui5/cli",
    "start": "node server.mjs",
    "start:mockserver": "cd ./mockserver && cds watch",
    "start:local": "fiori run --config ./ui5-local.yaml --open test/flpSandboxMockServer.html",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo",
    "sonar": "npm run sonar:grunt",
    "sonar:grunt": "grunt sonar",
    "test": "npm run lint && npm run test:ci && npm run sonar",
    "lint": "eslint -f codeframe webapp",
    "test:ci": "karma start karma-ci.conf.js",
    "test:dev": "karma start"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling",
      "@sap/ux-ui5-fe-mockserver-middleware"
    ]
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
